var x: int = 10;
var y: int = 20;

print(1111);

# Caso 1: Condición Verdadera (Solo IF)
if (x < y) {
    print(1); # Debe imprimirse
}

# Caso 2: Condición Falsa (Solo IF)
if (x > y) {
    print(0); # NO debe imprimirse
}

# Caso 3: IF-ELSE (Rama True)
if (x == 10) {
    print(2); # Debe imprimirse
} else {
    print(0); # NO debe imprimirse
}

# Caso 4: IF-ELSE (Rama Else)
if (x == 99) {
    print(0); # NO debe imprimirse
} else {
    print(3); # Debe imprimirse
}

print(9999);