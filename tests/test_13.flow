var x: int = 0;

match (x) {
    0: {
        print(0);
        x += 2;
    }
    1: {
        print(1);
        stop;
    }
    2: {
        print(2);
        x += 1;
        stop;
    }
    3: {
        print(3);
        stop;
    }
    _: {
        print(-1);
        stop;
    }
}

# Resultado
#
# 0
# 2
